<div class="container-fluid">
  <div class="row-fluid">
    <div class="span10 offset1">
      <h1 class="featurette-heading-blue">Create an Event</h1>
    </div>
  </div>
  <div class="row-fluid">
    <div class="span7 offset1">

      <p class="paragraph-font">
        Create an event for Blueprint.
      </p>
      <br>
      <%= form_for(@event, :url => {:action => 'create'}) do |form| %>

			<%= render(:partial => 'form', :locals => {:form => form}) %>

      <%= form.submit "Create Event" %>
      <% end %>

    </div>
  </div><!--/.row-fluid -->
<hr class="featurette-divider">
</div><!--/.container -->